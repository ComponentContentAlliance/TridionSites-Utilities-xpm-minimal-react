{
  "name": "headless-xpm-react",
  "version": "2.0.4",
  "description": "A reusable wrapper button to make Tridion sites react app page components editable.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs"
  },
  "author": "RWS",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "cpx": "^1.5.0",
    "rollup": "^4.18.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ComponentContentAlliance/TridionSites-Utilities-xpm-minimal-react"
  },
  "keywords": [
    "react",
    "cms",
    "wrapper",
    "editor",
    "component"
  ]
}
